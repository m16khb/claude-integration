    "workflow_patterns": {
      "엔티티 + 테스트": {
        "strategy": "SEQUENTIAL",
        "agents": ["typeorm-expert", "suites-testing-expert"]
      },
      "캐시 + 큐": {
        "strategy": "PARALLEL",
        "agents": ["redis-cache-expert", "bullmq-queue-expert"]
      },
      "코드 작성 완료": {
        "strategy": "SEQUENTIAL",
        "agents": ["code-reviewer", "test-automator"]
      },
      "마이크로서비스 + CQRS": {
        "strategy": "SEQUENTIAL",
        "agents": ["cqrs-expert", "microservices-expert"]
      }
    },
